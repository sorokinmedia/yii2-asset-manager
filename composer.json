{
  "name": "sorokinmedia/yii2-asset-manager",
  "description": "Asset manager for Yii2",
  "type": "yii2-extension",
  "keywords": ["yii2", "yii 2", "asset", "assets", "deploy"],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Ruslan Gilyazetdinov",
      "email": "ma3obblu@gmail.com",
      "homepage": "https://vk.com/ma3obblu"
    }
  ],
  "support": {
    "issues": "https://github.com/sorokinmedia/yii2-asset-manager/issues?state=open",
    "source": "https://github.com/sorokinmedia/yii2-asset-manager"
  },
  "require": {
    "yiisoft/yii2": ">=2.0.6"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "autoload": {
    "psr-4": {
      "sorokinmedia\\asset_manager\\": "src/"
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}