{
  "name": "sorokinmedia/yii2-asset-manager",
  "description": "Asset manager for Yii2",
  "type": "yii2-extension",
  "keywords": ["yii2", "yii 2", "asset", "assets", "deploy"],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Ruslan Gilyazetdinov",
      "email": "ma3obblu@gmail.com",
      "homepage": "https://www.facebook.com/Ma3oBblu"
    }
  ],
  "support": {
    "issues": "https://github.com/sorokinmedia/yii2-asset-manager/issues?state=open",
    "source": "https://github.com/sorokinmedia/yii2-asset-manager"
  },
  "require": {
    "yiisoft/yii2": ">=2.0.30"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "8.*"
  },
  "autoload": {
    "psr-4": {
      "sorokinmedia\\asset_manager\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
